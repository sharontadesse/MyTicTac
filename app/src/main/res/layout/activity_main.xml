<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    android:id="@+id/tic"
    android:clickable="true">

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/A2"
        android:backgroundTint="#33b5e5"
        android:layout_below="@+id/bNewGame"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="69dp" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/A3"
        android:backgroundTint="#33b5e5"
        android:layout_above="@+id/B3"
        android:layout_toRightOf="@+id/A2"
        android:layout_toEndOf="@+id/A2" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/B2"
        android:layout_below="@+id/A2"
        android:layout_alignLeft="@+id/A2"
        android:layout_alignStart="@+id/A2"
        android:backgroundTint="#33b5e5" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/B3"
        android:backgroundTint="#33b5e5"
        android:layout_alignTop="@+id/B2"
        android:layout_toRightOf="@+id/B2"
        android:layout_toEndOf="@+id/B2" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/A1"
        android:layout_above="@+id/B2"
        android:layout_toLeftOf="@+id/A2"
        android:layout_toStartOf="@+id/A2"
        android:backgroundTint="#33b5e5" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/B1"
        android:layout_above="@+id/C1"
        android:layout_toLeftOf="@+id/B2"
        android:layout_toStartOf="@+id/B2"
        android:backgroundTint="#33b5e5" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/C1"
        android:layout_below="@+id/B2"
        android:layout_alignLeft="@+id/B1"
        android:layout_alignStart="@+id/B1"
        android:backgroundTint="#33b5e5" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/C2"
        android:backgroundTint="#33b5e5"
        android:layout_below="@+id/B2"
        android:layout_toRightOf="@+id/B1"
        android:layout_toEndOf="@+id/B1" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/C3"
        android:layout_alignBottom="@+id/C2"
        android:layout_alignLeft="@+id/B3"
        android:layout_alignStart="@+id/B3"
        android:backgroundTint="#33b5e5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Game"
        android:id="@+id/bNewGame"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
